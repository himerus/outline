{
  "name": "@phase2/outline",
  "version": "0.0.1",
  "developmentVersion": "0.0.1",
  "description": "Outline Component Design - Web Component Library by Phase2",
  "main": "dist/outline.js",
  "module": "dist/outline.js",
  "es2015": "dist/outline-legacy.js",
  "es2017": "dist/outline.js",
  "unpkg": "dist/outline.js",
  "private": true,
  "workspaces": [
    "./packages/*"
  ],
  "files": [
    "./packages/*/dist"
  ],
  "author": {
    "name": "Phase2 Technology",
    "url": "https://phase2technology.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phase2/outline.git"
  },
  "license": "BSD-3-Clause",
  "storybook_port": 6042,
  "scripts": {
    "prep": "npm-run-all -s prep:*",
    "prep:css": "node scripts/styles.js",
    "prestart": "yarn prep",
    "start": "yarn storybook",
    "prebuild": "yarn prep",
    "build": "turbo run build",
    "test": "turbo run test",
    "package": "turbo run package",
    "lint": "npm-run-all -s lint:eslint lint:prettier",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "format": "npm-run-all -s format:*",
    "format:code": "eslint --fix . && prettier --write .",
    "fix": "yarn format && yarn lint",
    "clean": "npm-run-all -s clean:*",
    "clean:dist": "rimraf dist && rimraf packages/**/dist",
    "clean:turbo": "rimraf packages/**/.turbo",
    "clean:storybook": "rimraf storybook-static",
    "clean:css": "rimraf packages/**/*.lit.*",
    "clean:maps": "rimraf packages/**/*.map",
    "clean:types": "rimraf packages/**/*.d.ts",
    "clean:modules": "rimraf ./node_modules packages/**/node_modules",
    "reset": "yarn clean && yarn install",
    "reboot": "yarn clean && yarn install && yarn start",
    "storybook": "start-storybook -p ${npm_package_storybook_port}",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "web-components",
    "typescript",
    "lit"
  ],
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@changesets/cli": "^2.24.3",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@open-wc/testing": "^3.0.0-next.2",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/web-components": "^6.5.10",
    "@types/estree": "^0.0.47",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@web/dev-server": "0.0.29",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/test-runner": "^0.13.15",
    "@web/test-runner-browserstack": "^0.4.2",
    "@web/test-runner-playwright": "^0.8.6",
    "autoprefixer": "10.4.0",
    "esbuild": "^0.15.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lit-html": "^2.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.8",
    "postcss-custom-properties": "12.1.8",
    "postcss-discard-comments": "5.1.0",
    "postcss-import": "14.0.2",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.1",
    "prettier": "^2.0.4",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "rimraf": "^3.0.2",
    "rsync": "^0.6.1",
    "svgo": "^2.8.0",
    "tailwindcss": "3.0.0",
    "ts-lit-plugin": "^1.2.1",
    "turbo": "^1.2.16"
  },
  "dependencies": {}
}
