{
  "name": "@phase2/outline-package-test",
  "version": "0.0.1",
  "developmentVersion": "0.0.1",
  "description": "Outline Component Design",
  "main": "dist/outline.js",
  "module": "dist/outline.js",
  "es2015": "dist/outline-legacy.js",
  "es2017": "dist/outline.js",
  "unpkg": "dist/outline.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "packages/*/dist"
  ],
  "author": {
    "name": "Phase2 Technology",
    "url": "https://phase2technology.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phase2/outline-package-test.git"
  },
  "license": "BSD-3-Clause",
  "sbport": 6042,
  "scripts": {
    "prep": "npm-run-all -s prep:css",
    "prep:css": "node scripts/styles.js",
    "prebuild": "yarn prep",
    "build": "turbo run build",
    "test": "turbo run test",
    "package": "turbo run package",
    "lint": "npm-run-all -s lint:*",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "format": "npm-run-all -s format:*",
    "format:code": "eslint --fix . && prettier --write .",
    "format:svg": "svgo -f ./src -r --config=svgo.config.js",
    "fix": "yarn format && yarn lint",
    "clean": "npm-run-all -s clean:dist clean:turbo clean:css",
    "clean:dist": "rimraf dist && rimraf packages/**/dist",
    "clean:turbo": "rimraf packages/**/.turbo",
    "clean:storybook": "rimraf storybook-static",
    "clean:css": "rimraf packages/**/*.lit.ts",
    "clean:modules": "rimraf node_modules",
    "reset": "yarn clean && yarn install",
    "reboot": "yarn clean && yarn install && yarn build"
  },
  "keywords": [
    "web-components",
    "typescript",
    "lit"
  ],
  "packageManager": "yarn@3.2.2",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@open-wc/testing": "^3.0.0-next.2",
    "@types/estree": "^0.0.47",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@web/dev-server": "0.0.29",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/test-runner": "^0.13.15",
    "@web/test-runner-browserstack": "^0.4.2",
    "@web/test-runner-playwright": "^0.8.6",
    "autoprefixer": "10.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.8",
    "postcss-custom-properties": "12.1.8",
    "postcss-discard-comments": "5.1.0",
    "postcss-import": "14.0.2",
    "prettier": "^2.0.4",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "rimraf": "^3.0.2",
    "rsync": "^0.6.1",
    "tailwindcss": "3.0.0",
    "ts-lit-plugin": "^1.2.1",
    "turbo": "^1.2.16",
    "wait-on": "^5.3.0"
  }
}
